package org.spicefactory.parsley.core.registry.definition {
import org.spicefactory.parsley.core.registry.definition.ObjectInstantiator;

/**
 * Marker interface for instantiators that are required by the container and cannot be overwritten for 
 * an ObjectDefinition. An example are instantiators for simple MXML configuration tags where the object creation
 * code gets generated by the Flex compiler and Parsley has no control over object creation.
 * 
 * @author Jens Halm
 */
public interface ContainerObjectInstantiator extends ObjectInstantiator {
}
}
