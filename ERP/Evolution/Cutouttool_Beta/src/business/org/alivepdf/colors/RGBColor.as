package business.org.alivepdf.colors

{
	
	public final class RGBColor implements Color
	
	{
		
		public var r:Number;
		public var g:Number;
		public var b:Number;
		
		public function RGBColor ( pRed:Number, pGreen:Number, pBlue:Number )
		
		{
			
			r = pRed;
			g = pGreen;
			b = pBlue;
			
		}
		
	}
	
}