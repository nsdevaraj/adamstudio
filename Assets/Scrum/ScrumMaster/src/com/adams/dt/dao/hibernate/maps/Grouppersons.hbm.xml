<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.adams.dt.pojo.Grouppersons" table="group_persons">
        <id name="groupPersonId" type="integer" column="group_person_ID">
            <generator class="increment" />
        </id>
        <property name="groupFk">
            <column name="group_FK" />
        </property>
        <property name="personFk">
            <column name="person_FK" />
        </property>
    </class> 
</hibernate-mapping>
